      }
      
      .container {
        max-width: 800px;
        margin: 20px auto;
        padding: 20px;
        background-color: #fff;
        box-shadow: 0 0 10px rgba(0, 0, 0, 0.1);
        border-radius: 5px;
      }
      
      h2 {
        color: #333;
        text-align: center;
      }
      
      .user-info {
        margin-bottom: 20px;
        padding: 15px;
